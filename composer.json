{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "bensampo/laravel-enum": "^5.3",
        "creativeorange/gravatar": "^1.0",
        "ericlagarda/nova-embed": "^2.1",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^7.2",
        "honeybadger-io/honeybadger-laravel": "^3.14",
        "kyslik/column-sortable": "^6.4",
        "laraning/nova-time-field": "^1.2",
        "laravel-notification-channels/discord": "^1.3",
        "laravel/framework": "^9.35",
        "laravel/nova": "^3.31",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.5",
        "skydiver/nova-update-card": "^1.5",
        "socialiteproviders/discord": "^4.1",
        "spatie/laravel-activitylog": "^4.6",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-permission": "^5.5",
        "themsaid/wink": "^1.3",
        "tucker-eric/eloquentfilter": "^3.1",
        "vyuldashev/nova-permission": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.14",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
